{
  "processName": "testProc",
  "globalValue": {
    "url": "https://www.boy5.com",
    "encoding": "utf8",
    "searchkey": "围城"
  },
  "beginSegment": [
    {
      "action": "getValue",
      "exp": "{android.applicationDir} - hello world"
    },
    {
      "action": "pause"
    },
    {
      "action": "getHtml",
      "url": "{url}/modules/article/search.php?searchkey={searchkey}",
      "method": "get",
      "charset": "utf8"
    },
    {
      "action": "callMultiProcess",
      "values": "{this}",
      "multiProcess": [
        {
          "action": "multiSelector",
          "type": "regexp",
          "script": "src=(\"[^\"]*\")"
        }
      ]
    },
    {
      "action": "pause"
    },
    {
      "action": "callMultiProcess",
      "multiBuilder":[
        {
          "action": "fill",
          "valueName": "ipage",
          "type": "list",
          "list": [1,2,4,5,7],
          "exp": "{url}_{ipage}"
        }
       ],
       "values": [],
       "multiProcess": [

       ]
    }

  ]
}